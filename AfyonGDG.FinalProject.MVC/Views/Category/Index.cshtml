@model List<Category>


<table class="table">
    <thead>
        <tr>
            <th scope="col">Kategori Id</th>
            <th scope="col">Kategori Adı</th>
        </tr>
    </thead>
    <tbody>

        @foreach (Category item in Model)
        {
            <tr>
                <th scope="row">@item.Id</th>
                <td>@item.Name</td>
            </tr>
        }
   

    </tbody>
</table>



