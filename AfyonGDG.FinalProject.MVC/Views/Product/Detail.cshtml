@model ProductViewModel

<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">@Model.Name</h5>
    <h6 class="card-subtitle mb-2 text-body-secondary">Kategori:  @Model.CategoryName</h6>
    <p class="card-text">@Model.Description</p>
        <p class="card-text ">Stok Adedi :  @Model.Stock</p>
        <p class="card-text ">Ürün Değeri :  @Model.Price</p>
  </div>
</div>
